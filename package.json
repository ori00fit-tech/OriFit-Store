{
  "name": "orifit-store",
  "version": "1.0.0",
  "description": "OriFit Store - Cloudflare Workers API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy src/index.ts",
    "deploy:prod": "wrangler deploy src/index.ts --env production",
    "build": "wrangler publish src/index.ts --dry-run",
    "db:schema": "wrangler d1 execute orifit-db --file schema.sql",
    "db:schema:dev": "wrangler d1 execute orifit-db-dev --file schema.sql --env development"
  },
  "dependencies": {
    "itty-router": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  }
}
